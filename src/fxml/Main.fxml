            <JFXButton fx:id="ebtn" buttonType="RAISED" layoutX="21.0" layoutY="429.0" onAction="#exitTypePressed" text="Emergency" textFill="WHITE" AnchorPane.bottomAnchor="110.0" AnchorPane.leftAnchor="20.0">
